  <script type="text/javascript">
	$(function(){
		getConfDatas();
		
		openOnlineLayout();
		
	});
	
	function openOnlineLayout(){
		var _fileName=$('#_fileName').html();
			$.ajax({
				url: MISC_FILE,
				type: "post",
				dataType:'html',
				data:{param:'file',path:_fileName},
				success: function (data) {
					$(".demo").html(data);
					initContainer();
				}
			});
	}
	function saveOnlineLayout(){
		var _fileName=$('#_fileName').html();
		var cont= $(".demo").html();
			$.ajax({
				url: MISC_FILE,
				type: "post",
				dataType:'json',
				data:{param:'save',path:_fileName,cont:cont},
				success: function (data) {
					clearDemo(true);
					alert('保存成功!');
				}
			});
	}

  </script>
