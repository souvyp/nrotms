
{
  "namespace": "data",
  "cmd": "sqlcmd",
  "id": "rms_0068",
  "version": 1,
  "paras": {
    "dbtype": "sqlserver",
    "sql": "SELECT TOP 10 dbo.fn_pub_order_AreaName(Index_ToCity) AS [toArea],dbo.fn_pub_order_AreaLocation(Index_ToCity,0) AS [To],SUM(1) AS [Total],SUM(dbo.fn_pub_order_TotalWeight(Index_ID,1)) AS [TotalWeight],SUM(dbo.fn_pub_order_TotalVolume(Index_ID,1)) AS [TotalVolume],SUM(dbo.fn_pub_order_TotalCost(Index_ID)) AS [TotalCost],SUM(dbo.fn_pub_order_TotalAmount(Index_ID,0)) AS [TotalAmount] FROM TMS_OrderIndex AS S  WHERE Index_CreatorCompanyID = dbo.fn_pub_user_User2CompanyID(@operator,0)  AND Index_SrcClass = 1 AND Index_Invalid = 0 AND Index_SrcOrderID > 0 AND Index_Status <> 32 AND YEAR(Index_CreateTime)=YEAR(GETDATE()) GROUP BY Index_ToCity ORDER BY [TotalVolume] DESC",
    "readonly": "false",
    "type": "text",
    "paras": [
      {
        "name": "operator",
        "type": "bigint",
        "size": 8,
        "direction": "input",
   
      }
    ],
